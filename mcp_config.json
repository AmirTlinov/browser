{
  "servers": {
    "browser": {
      "enabled": true,
      "transport": {
        "type": "stdio",
        "command": "./scripts/run_browser_mcp.sh",
        "args": [],
        "cwd": ".",
        "env": {
          "MCP_QUIET": "1",
          "MCP_TOOLSET": "full",
          "MCP_BROWSER_MODE": "extension",
          "MCP_AUTO_PORT_FALLBACK": "0",
          "MCP_DUMP_FRAMES": "data/outbox/mcp_frames.log"
        }
      }
    }
  }
}
